{
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "php-cs-fixer.lastDownload": 1618852336390,
    "diffEditor.renderSideBySide": true,
    "explorer.openEditors.visible": 0,
    "explorer.decorations.badges": false,
